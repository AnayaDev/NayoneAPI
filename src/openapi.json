{
  "openapi": "1.0.0",
  "info": {
    "title": "Nayone",
    "author": "NAYONE",
    "version": "v1.0.0",
    "description": "Simple and easy to use API."
  },
  "servers": [
    { "url": "/" }
  ],
  "tags": [
    { "name": "AI" },
    { "name": "Anime" },
    { "name": "Downloader" },
    { "name": "Image" },
   { "name": "News" },
  { "name": "Stalk" }
  ],
  "paths": {
"/news/jadwalsholat": {
  "get": {
    "summary": "Jadwal Sholat Harian",
    "description": "Mengambil jadwal sholat hari ini untuk kota/daerah di Indonesia. Data real-time dari Aladhan API.",
    "tags": ["News"],
    "parameters": [
      {
        "name": "kota",
        "in": "query",
        "required": false,
        "schema": {
          "type": "string",
          "example": "Jakarta"
        },
        "description": "Nama kota/daerah di Indonesia. Default: Jakarta"
      }
    ],
    "responses": {
      "200": {
        "description": "Berhasil mengambil jadwal sholat hari ini",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "boolean",
                  "example": true
                },
                "creator": {
                  "type": "string",
                  "example": "Nayone API"
                },
                "source": {
                  "type": "string",
                  "example": "Aladhan API"
                },
                "result": {
                  "type": "object",
                  "properties": {
                    "tanggal": {
                      "type": "string",
                      "example": "15-02-2026"
                    },
                    "hijriyah": {
                      "type": "string",
                      "example": "05-08-1447"
                    },
                    "kota": {
                      "type": "string",
                      "example": "Jakarta"
                    },
                    "imsak": {
                      "type": "string",
                      "example": "04:33"
                    },
                    "subuh": {
                      "type": "string",
                      "example": "04:43"
                    },
                    "dzuhur": {
                      "type": "string",
                      "example": "11:45"
                    },
                    "ashar": {
                      "type": "string",
                      "example": "15:10"
                    },
                    "maghrib": {
                      "type": "string",
                      "example": "18:05"
                    },
                    "isya": {
                      "type": "string",
                      "example": "19:15"
                    }
                  }
                }
              }
            }
          }
        }
      },
      "500": {
        "description": "Terjadi kesalahan pada server",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": { "type": "boolean", "example": false },
                "error": { "type": "string", "example": "Gagal mengambil data dari Aladhan API" }
              }
            }
          }
        }
      }
    }
  }
                },    
"/news/jadwalpuasa": {
  "get": {
    "summary": "Jadwal Imsak & Buka Puasa (Auto Detect Ramadan)",
    "description": "Mengambil jadwal imsak, subuh, dan buka puasa berdasarkan kota dengan auto detect bulan Ramadan secara otomatis.",
    "tags": ["News"],
    "parameters": [
      {
        "name": "kota",
        "in": "query",
        "required": false,
        "schema": {
          "type": "string",
          "example": "Jakarta"
        },
        "description": "Nama kota di Indonesia. Default: Jakarta"
      },
      {
        "name": "tahun",
        "in": "query",
        "required": false,
        "schema": {
          "type": "integer",
          "example": 2026
        },
        "description": "Tahun yang ingin dicek. Default: tahun sekarang"
      },
      {
        "name": "today",
        "in": "query",
        "required": false,
        "schema": {
          "type": "boolean",
          "example": true
        },
        "description": "Jika true, hanya menampilkan jadwal hari ini saja"
      }
    ],
    "responses": {
      "200": {
        "description": "Berhasil mengambil jadwal Ramadan",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "boolean",
                  "example": true
                },
                "creator": {
                  "type": "string",
                  "example": "Nayone API"
                },
                "source": {
                  "type": "string",
                  "example": "Aladhan API"
                },
                "kota": {
                  "type": "string",
                  "example": "Jakarta"
                },
                "tahun": {
                  "type": "integer",
                  "example": 2026
                },
                "ramadan_bulan_masehi": {
                  "type": "integer",
                  "example": 3
                },
                "total": {
                  "type": "integer",
                  "example": 30
                },
                "result": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "tanggal_masehi": {
                        "type": "string",
                        "example": "01-03-2026"
                      },
                      "tanggal_hijriyah": {
                        "type": "string",
                        "example": "1-09-1447"
                      },
                      "imsak": {
                        "type": "string",
                        "example": "04:33"
                      },
                      "subuh": {
                        "type": "string",
                        "example": "04:43"
                      },
                      "maghrib": {
                        "type": "string",
                        "example": "18:05"
                      },
                      "buka_puasa": {
                        "type": "string",
                        "example": "18:05"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "500": {
        "description": "Terjadi kesalahan pada server"
      }
    }
  }
},    
"/news/liburnasional": {
  "get": {
    "summary": "Libur Nasional & Cuti Bersama Indonesia",
    "description": "Mengambil daftar tanggal merah, libur nasional, dan cuti bersama berdasarkan tahun dan bulan tertentu.",
    "tags": ["News"],
    "parameters": [
      {
        "name": "tahun",
        "in": "query",
        "required": false,
        "schema": {
          "type": "integer",
          "example": 2026
        },
        "description": "Tahun yang ingin dicek. Default: tahun sekarang"
      },
      {
        "name": "bulan",
        "in": "query",
        "required": false,
        "schema": {
          "type": "integer",
          "minimum": 1,
          "maximum": 12,
          "example": 1
        },
        "description": "Filter berdasarkan bulan (1 = Januari, 12 = Desember)"
      },
      {
        "name": "tipe",
        "in": "query",
        "required": false,
        "schema": {
          "type": "string",
          "enum": ["merah", "semua"],
          "example": "merah"
        },
        "description": "Filter hanya tanggal merah atau semua jenis libur"
      },
      {
        "name": "upcoming",
        "in": "query",
        "required": false,
        "schema": {
          "type": "boolean",
          "example": true
        },
        "description": "Jika true, hanya menampilkan libur yang belum lewat"
      }
    ],
    "responses": {
      "200": {
        "description": "Berhasil mengambil data libur",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "boolean",
                  "example": true
                },
                "creator": {
                  "type": "string",
                  "example": "Nayone API"
                },
                "source": {
                  "type": "string",
                  "example": "Nager.Date + Cuti Bersama Manual"
                },
                "tahun": {
                  "type": "integer",
                  "example": 2026
                },
                "bulan": {
                  "type": "string",
                  "example": "Januari"
                },
                "total": {
                  "type": "integer",
                  "example": 3
                },
                "result": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "tanggal": {
                        "type": "string",
                        "example": "2026-01-01"
                      },
                      "nama": {
                        "type": "string",
                        "example": "Tahun Baru Masehi"
                      },
                      "nama_internasional": {
                        "type": "string",
                        "example": "New Year's Day"
                      },
                      "tipe": {
                        "type": "string",
                        "example": "Libur Nasional"
                      },
                      "kategori": {
                        "type": "string",
                        "example": "tanggal_merah"
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "500": {
        "description": "Terjadi kesalahan pada server"
      }
    }
  }
                    },    
"/news/cuaca": {
  "get": {
    "summary": "Info Cuaca Berdasarkan Nama Wilayah (Global)",
    "description": "Mengambil data cuaca terkini berdasarkan nama kota menggunakan Open-Meteo. Jika parameter tidak diisi, default Jakarta.",
    "tags": ["News"],
    "parameters": [
      {
        "name": "wilayah",
        "in": "query",
        "required": false,
        "schema": {
          "type": "string",
          "example": "Jakarta"
        },
        "description": "Nama kota (contoh: Jakarta, Tokyo, London). Default: Jakarta"
      }
    ],
    "responses": {
      "200": {
        "description": "Berhasil mengambil data cuaca",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "boolean",
                  "example": true
                },
                "creator": {
                  "type": "string",
                  "example": "Nayone API"
                },
                "source": {
                  "type": "string",
                  "example": "Open-Meteo"
                },
                "result": {
                  "type": "object",
                  "properties": {
                    "lokasi": {
                      "type": "object",
                      "properties": {
                        "nama": { "type": "string" },
                        "negara": { "type": "string" },
                        "latitude": { "type": "number" },
                        "longitude": { "type": "number" }
                      }
                    },
                    "cuaca": {
                      "type": "object",
                      "properties": {
                        "waktu": { "type": "string" },
                        "suhu": { "type": "string", "example": "30 °C" },
                        "kecepatan_angin": { "type": "string", "example": "15 km/jam" },
                        "arah_angin": { "type": "string", "example": "120°" }
                      }
                    }
                  }
                }
              }
            }
          }
        }
      },
      "500": {
        "description": "Gagal memproses request atau wilayah tidak ditemukan"
      }
    }
  }
},
"/news/gempa": {
  "get": {
    "summary": "Info Gempa Indonesia",
    "description": "Mengambil data gempa terbaru atau daftar gempa berdasarkan filter magnitude dan limit dari BMKG",
    "tags": ["News"],
    "parameters": [
      {
        "name": "min",
        "in": "query",
        "required": false,
        "schema": {
          "type": "number",
          "example": 5
        },
        "description": "Filter minimal magnitude (contoh: 5)"
      },
      {
        "name": "limit",
        "in": "query",
        "required": false,
        "schema": {
          "type": "integer",
          "example": 5
        },
        "description": "Jumlah data gempa yang ditampilkan (default: 5)"
      }
    ],
    "responses": {
      "200": {
        "description": "Berhasil mengambil data gempa",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "status": {
                  "type": "boolean",
                  "example": true
                },
                "creator": {
                  "type": "string",
                  "example": "Nayone API"
                },
                "source": {
                  "type": "string",
                  "example": "BMKG"
                },
                "result": {
                  "oneOf": [
                    {
                      "type": "object",
                      "properties": {
                        "waktu": {
                          "type": "object",
                          "properties": {
                            "tanggal": { "type": "string" },
                            "jam": { "type": "string" },
                            "dateTime": { "type": "string" }
                          }
                        },
                        "lokasi": {
                          "type": "object",
                          "properties": {
                            "wilayah": { "type": "string" },
                            "lintang": { "type": "string" },
                            "bujur": { "type": "string" },
                            "coordinates": { "type": "string" }
                          }
                        },
                        "kekuatan": {
                          "type": "object",
                          "properties": {
                            "magnitude": { "type": "number" },
                            "kedalaman": { "type": "string" },
                            "potensi": { "type": "string" },
                            "dirasakan": { "type": "string" }
                          }
                        },
                        "shakemap": {
                          "type": "string",
                          "format": "uri",
                          "nullable": true
                        }
                      }
                    },
                    {
                      "type": "array",
                      "items": {
                        "type": "object",
                        "properties": {
                          "waktu": { "type": "object" },
                          "lokasi": { "type": "object" },
                          "kekuatan": { "type": "object" },
                          "shakemap": {
                            "type": "string",
                            "format": "uri",
                            "nullable": true
                          }
                        }
                      }
                    }
                  ]
                }
              }
            }
          }
        }
      },
      "500": {
        "description": "Gagal memproses request"
      }
    }
  }
                      },    
